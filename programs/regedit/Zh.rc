IDR_REGEDIT_MENU MENU DISCARDABLE
BEGIN
    POPUP "註冊表(&R)"
    BEGIN
        MENUITEM "導入註冊表檔案(&I)...", ID_REGISTRY_IMPORTREGISTRYFILE
        MENUITEM "導出註冊表檔案(&E)...", ID_REGISTRY_EXPORTREGISTRYFILE
