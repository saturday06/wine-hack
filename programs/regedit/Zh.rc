        MENUITEM "改名(&R)\tF2", ID_EDIT_RENAME
        MENUITEM SEPARATOR
        MENUITEM "複製關鍵詞名稱(&C)", ID_EDIT_COPYKEYNAME
        MENUITEM SEPARATOR
        MENUITEM "查找(&F)\tCtrl+F", ID_EDIT_FIND, GRAYED
