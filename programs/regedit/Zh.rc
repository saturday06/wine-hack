        MENUITEM "狀態欄(&B)", ID_VIEW_STATUSBAR
        MENUITEM SEPARATOR
        MENUITEM "切開(&L)", ID_VIEW_SPLIT
        MENUITEM SEPARATOR
        MENUITEM "刷新(&R)\tF5", ID_VIEW_REFRESH
    END
