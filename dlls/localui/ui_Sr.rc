    EDITTEXT LPTCONFIG_EDIT, 112, 20, 32, 13, WS_VISIBLE | ES_NUMBER
    DEFPUSHBUTTON "U redu", IDOK, 164, 10, 50, 14, WS_VISIBLE
    PUSHBUTTON "Otkaži", IDCANCEL, 164, 27, 50, 14, WS_VISIBLE
